package mathclass;

public class ComplexArimethic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int a = 3;
int b = 23;
int c = 2;
int d = 50;
float e;
double arithmetic;

e = (float)  d/6;
arithmetic = (-b + Math.sqrt(b * b - 4 * a * c))/ (2 * a);


System.out.print("The arimetic is ");
System.out.printf("%.2f", arithmetic);
System.out.println();
System.out.println(e);
	}

}
