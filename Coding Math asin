package mathclass;

public class Mathasin {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double a = 30;
		double b = Math.toRadians(a);
 
        System.out.println(Math.sin(b));
 
        a = 45;
        b = Math.toRadians(a);
 
        System.out.println(Math.sin(b));
 
        a = 60;
        b = Math.toRadians(a);
 
        System.out.println(Math.sin(b));
 
        a = 90;
        b = Math.toRadians(a);
 
        System.out.println(Math.sin(b));
	}

}
