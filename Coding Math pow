package mathclass;
import java.lang.Math;
public class Mathpow {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double a = 30;
        double b = 2;
        System.out.println(Math.pow(a, b));
  
        a = 3;
        b = 4;
        System.out.println(Math.pow(a, b));
  
        a = 2;
        b = 6;
        System.out.println(Math.pow(a, b));
	}

}
