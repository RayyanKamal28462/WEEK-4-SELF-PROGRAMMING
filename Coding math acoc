package mathclass;

public class Mathacos {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 double a = 30;
		 double b = Math.toRadians(a);
	  
	        System.out.println(Math.cos(b));
	  
	        a = 45;
	        b = Math.toRadians(a);
	  
	        System.out.println(Math.cos(b));
	  
	        a = 60;
	        b = Math.toRadians(a);
	  
	        System.out.println(Math.cos(b));
	  
	        a = 0;
	        b = Math.toRadians(a);
	  
	        System.out.println(Math.cos(b));
	}

}
